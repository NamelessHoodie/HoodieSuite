<Window x:Class="ToolName.MVVM.View.ParamStudioTestInterop"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:ToolName.MVVM.View"
        mc:Ignorable="d"
        Title="ParamStudioTestInterop" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
            <RowDefinition Height="20*" />
        </Grid.RowDefinitions>
        <Button Grid.Row="0" Click="StartClient_OnClick" Content="ActivateServer" />
        <Button Grid.Row="1" Click="StartParamStudio_OnClick" Content="LaunchParamStudio" />
    </Grid>
</Window>
